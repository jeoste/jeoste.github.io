---
import Layout from "@/layouts/Layout.astro";
import Header from "@/components/Header.astro";
import Footer from "@/components/Footer.astro";
import Main from "@/layouts/Main.astro";
import { SITE } from "@/config";

const pageTitle = "Portfolio";
const pageDesc = "Discover my projects and achievements";
---

<Layout>
  <Header />
  <Main pageTitle={pageTitle} pageDesc={pageDesc}>
    <div class="prose prose-lg prose-slate dark:prose-invert">
      <p>Welcome to my portfolio. This section showcases my projects and achievements.</p>
      <p>Content coming soon...</p>
    </div>
  </Main>
  <Footer />
</Layout> 