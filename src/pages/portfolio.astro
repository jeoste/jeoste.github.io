---
import Layout from "@/layouts/Layout.astro";
import Header from "@/components/Header.astro";
import Footer from "@/components/Footer.astro";
import Main from "@/layouts/Main.astro";
import { SITE } from "@/config";

const pageTitle = "Portfolio";
const pageDesc = "Découvrez mes projets et réalisations";
---

<Layout>
  <Header />
  <Main pageTitle={pageTitle} pageDesc={pageDesc}>
    <div class="prose prose-lg prose-slate dark:prose-invert">
      <p>Bienvenue dans mon portfolio. Cette section présente mes projets et réalisations.</p>
      <p>Contenu à venir...</p>
    </div>
  </Main>
  <Footer />
</Layout> 